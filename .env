```env
# Audio Player with Visualizer Environment Configuration
VITE_APP_NAME=Audio Player with Visualizer
VITE_APP_VERSION=1.0.0

# Audio Configuration
VITE_AUDIO_SAMPLE_RATE=44100
VITE_AUDIO_FFT_SIZE=2048
VITE_AUDIO_SMOOTHING_TIME_CONSTANT=0.8

# Visualizer Settings
VITE_VISUALIZER_CANVAS_WIDTH=800
VITE_VISUALIZER_CANVAS_HEIGHT=400
VITE_VISUALIZER_BAR_COUNT=64
VITE_VISUALIZER_BAR_GAP=2

# External Audio Services (Optional)
# VITE_FREESOUND_API_KEY=your_freesound_api_key_here
# VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id_here
# VITE_SOUNDCLOUD_CLIENT_ID=your_soundcloud_client_id_here

# CDN URLs for Sample Audio Files
VITE_AUDIO_CDN_BASE_URL=https://www.soundjay.com/misc/sounds-of-nature
VITE_FALLBACK_AUDIO_URL=https://www.soundjay.com/misc/beep-07a.wav

# Performance Settings
VITE_AUDIO_BUFFER_SIZE=4096
VITE_VISUALIZER_ANIMATION_FPS=60
VITE_AUDIO_CROSSFADE_DURATION=300

# Theme Configuration
VITE_DEFAULT_THEME=dark
VITE_ENABLE_GLASSMORPHISM=true
VITE_ENABLE_PARTICLE_EFFECTS=true

# Development Settings
VITE_ENABLE_AUDIO_DEBUG=false
VITE_SHOW_PERFORMANCE_METRICS=false
VITE_ENABLE_KEYBOARD_SHORTCUTS=true

# Browser Compatibility
VITE_FALLBACK_TO_HTML5_AUDIO=true
VITE_ENABLE_MOBILE_OPTIMIZATIONS=true
VITE_TOUCH_GESTURE_SENSITIVITY=0.8
```